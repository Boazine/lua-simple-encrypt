local gg = gg

function main()
menu = gg.multiChoice({
    'GATES IN SHIETOR'},
nil,'SCRIPT BY CID TEMPEST')
if menu == nil then os.exit() end
if menu[1] then g4() end
if menu[2] then g5() end
if menu[3] then g6() end
if menu[4] then g7() end
end

function g4()
gg.clearResults()
gg.setVisible(false)
gg.setRanges(gg.REGION_JAVA_HEAP)
gg.searchNumber("836;1008981770::50", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.processResume()
gg.refineNumber("1008981770", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
revert = gg.getResults(100, nil, nil, nil, nil, nil, nil, nil, nil)
gg.editAll("-1", gg.TYPE_DWORD)

gg.toast('Next')
gg.clearResults()
gg.processResume()
end

